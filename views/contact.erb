<form id="contact" action='contact' method='POST'>
	<legend>Primary Technical Contact:</legend>

		<div class="form-group">
			<label>Do you have an existing Skyscape service or account?</label>
			<div class="radio">
				<label><input type="radio" name="contact[existing_acct]">Yes</label>
			</div>
			<div class="radio">
				<label><input type="radio" name="contact[existing_acct]">No</label>
			</div>
			<br>
			<label>First Name</label>
			<input type="text" class="form-control" name="contact[firstname]">
			
			<label>Last Name</label>
			<input type="text" class="form-control" name="contact[lastname]">
			
			<label>Mobile Number</label>
			<input type="text" class="form-control" name="contact[mobile]">
			<button type="submit" class="btn">Next</button>
		</div>
</form>
			
		
			
		
